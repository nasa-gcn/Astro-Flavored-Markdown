{
  "name": "astro-flavored-markdown",
  "version": "1.0.0",
  "description": "A remark markdown dialect for astronomical bulletins",
  "main": "index.js",
  "scripts": {
    "prepare": "tsc",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "repository": {
    "type": "git",
    "url": "github:nasa-gcn/astro-flavored-markdown"
  },
  "keywords": [],
  "contributors": [
    {
      "name": "Dakota Dutko",
      "email": "dakota.c.dutko@nasa.gov"
    },
    {
      "name": "Leo Singer",
      "email": "leo.p.singer@nasa.gov"
    }
  ],
  "dependencies": {
    "unist-util-visit": "^4.1.1"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.3",
    "@types/jest": "^29.2.5",
    "@types/mdast": "^3.0.10",
    "@types/unist": "^2.0.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "jest": "^29.3.1",
    "mdast-util-from-markdown": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.2",
    "ts-jest": "^29.0.4",
    "typescript": "^4.9.4"
  },
  "type": "module",
  "sideEffects": false,
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "preset": "ts-jest",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  }
}
